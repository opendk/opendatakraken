<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="opendatakraken-repo" transaction-type="RESOURCE_LOCAL">
		<class>org.opendatakraken.meta.entity.StageColumn</class>
		<class>org.opendatakraken.meta.entity.StageColumnCheck</class>
		<class>org.opendatakraken.meta.entity.StageObject</class>
		<class>org.opendatakraken.meta.entity.StageSource</class>
		<class>org.opendatakraken.meta.entity.StageSourceDb</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="driver=com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/dbadmin"/>
			<property name="javax.persistence.jdbc.user" value="dbadmin"/>
			<property name="javax.persistence.jdbc.password" value="dbadmin"/>
		</properties>
	</persistence-unit>
</persistence>
